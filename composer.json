{
    "name": "haldayne/prybar",
    "description": "Bypass OOP visibility rules so you can get, set, and call private or protected members.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "require": {
        "php": "^5.6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Haldayne\\Prybar\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Haldayne\\Prybar\\": "tests/"
        }
    }
}
